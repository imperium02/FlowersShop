{
  "name": "ps-redux",
  "description": "React and Redux Pluralsight course by Cory House",
  "scripts": {
    "start": "webpack serve --config webpack.config.dev.js --port 3000"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "immer": "9.0.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
    "react-router-dom": "5.2.0",
    "react-toast-notifications": "^2.4.3",
    "react-toastify": "7.0.3",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.2.2",
    "babel-preset-react-app": "10.0.0",
    "css-loader": "5.2.0",
    "cssnano": "4.1.10",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.23.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-webpack-plugin": "^2.5.3",
    "fetch-mock": "9.11.0",
    "html-webpack-plugin": "5.3.1",
    "http-server": "0.12.3",
    "jest": "26.6.3",
    "json-server": "0.16.3",
    "mini-css-extract-plugin": "1.4.0",
    "miragejs": "^0.1.41",
    "node-fetch": "^2.6.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "5.2.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "17.0.2",
    "react-testing-library": "6.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.4",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "webpack": "5.30.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ],
    "plugins": [
      "react-refresh/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
